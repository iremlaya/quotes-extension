{
  "manifest_version": 3,
  "description": "Unmotivated Quotes is a chrome extension that displays dark quotes about life.",
  "version": "1.0",
  "name": "Unmotivated Quotes",
  "action": {
      "default_title": "Unmotivated Quotes"
  },
  "icons": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
  },
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"

  }
}